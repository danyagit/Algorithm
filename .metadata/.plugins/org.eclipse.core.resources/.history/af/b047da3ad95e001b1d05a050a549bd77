package com.xjf.algorithm.leetcode.shujujiegou.shu.BST;

//2. 寻找二叉查找树的第 k 个元素
public class BST02 {

	public static void main(String[] args) {

	}
	//中序遍历的第k个元素
	public int kthSmallest(TreeNode root, int k) {
		return dfs(root);
	}
	public int dfs(TreeNode root) {
		dfs(root.left);
		
		dfs(root.right);
	}
}
